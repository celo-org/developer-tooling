{
  "$schema": "https://typedoc.org/schema.json",
  "exclude": ["**/*.test.ts"],
  "excludePrivate": true,
  "excludeProtected": true,
  "hideGenerator": true,
  "out": "../docs/viem-account-ledger",
  "gitRevision": "master",
  "readme": "./README.md",
  "entryPoints": ["./src/index.ts"],
  "excludeInternal": true,
  "githubPages": false,
  "excludeReferences": true,
  "includeVersion": true,
  "plugin": ["typedoc-plugin-markdown"],
  "entryPointStrategy": "resolve",
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "rewrittenLink": false,
    "notDocumented": false,
    "unusedMergeModuleWith": false
  }
}
